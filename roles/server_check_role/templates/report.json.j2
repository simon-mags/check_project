{
  "server_report_data": [
    {% for host in vars['play_hosts'] %}
    {
      {% for field in report_field_list %}
      "{{ field.field_name }}": "{{ hostvars[host]['server_report_data'][field.field_name] | default('NOVALUE') | to_json }}",
      {% endfor %}

      {{ hostvars[host]['ansible_facts'] }}

    }
    {% if not loop.last %},{% endif %}
    {% endfor %}
  ],
  "check_project_version": "{{ check_project_version }}",
  "run_time": "{{ ansible_date_time.iso8601 }}"
}
